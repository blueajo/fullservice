{#
  Fallback template for all non-front pages.
  This ensures any URL besides '/' displays a "Page Not Found" message.
#}

{{ attach_library('fullservice/accessory-styling') }}
{{ attach_library('fullservice/accessory-scripts') }}

<div id="not-found-page">
  <div id="cursor">
    <div id="cursor-wrapper">
      <div id="cursor-dot">
      </div>
    </div>
  </div>
  <p>404 - Page Not Found</p>
  <p class='caption'>The page you are looking for does not exist.</p>
  <a class='caption' href="/">
    Go to Homepage
  </a>
</div>
